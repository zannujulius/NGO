<%- include('./partials/header.ejs') %> 

    <!-- breadcrumb start-->
    <section class="breadcrumb breadcrumb_bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb_iner">
                        <div class="breadcrumb_iner_item text-center">
                            <h2>blog</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- breadcrumb start-->

    <!--================Blog Area =================-->
    <div class="container">
        <section class="blog_area section_padding">
            <div class="container">
                <div class="row justify-content-between">
                    <% blog.forEach(blog => { %> 
                        <div class="col-lg-5 mb-5 mb-lg-0 ">
                            <article class="blog_item">
                                <div class="blog_item_img">
                                    <img class="card-img rounded-0" src="<%= blog.blogImage %> " alt="">
                                    
                                </div>

                                <div class="blog_details">
                                    <a class="d-inline-block" href="/blog/<%= blog._id %> ">
                                        <h2><%= blog.blogTitle %> </h2>
                                    </a>
                                    <p><%= blog.textOne %>  
                                        <a href="/blog/<%= blog._id %>" class="btn btn-sm btn-secondary text-white p-0 " style="color:white !important;padding:0 2px 0 2px !important;">
                                            more..
                                        </a>
                                    </p>
                                </div>  
                            </article>
                        </div>
                    <% }) %> 
                </div>
            </div>
        </section>
    </div>
